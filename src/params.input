[TimeLoop]
DtInitial = 1    # [s]
TEnd = 100000    # [s]

[Grid]
LowerLeft = 0 0
UpperRight = 2 1
Cells = 160 80

[Problem]
Name = Henry # name passed to the output routines
EnableGravity = true # enable gravity
Permeability = 1.00728E-9
Porosity = 0.35
Inflow = 6.6E-8

[Vtk]
AddVelocity = true # enable velocity output

[Newton]
ResidualReduction = 1E-10
